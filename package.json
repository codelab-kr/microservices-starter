{
  "name": "microservices-starter",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "prepare": "husky install",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:prod": "node start.prod.js",
    "start:dev": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config jest-e2e.json",
    "migration:generate": "cd database/mysql/ && NODE_ENV=migration ts-node ../../../node_modules/typeorm/cli.js migration:generate -d ../../../libs/common/src/database/mysql/mayql.data.source.ts",
    "migration:run": "cd database/mysql/migrations && NODE_ENV=migration ts-node ../../../node_modules/typeorm/cli.js migration:run -d ../../../libs/common/src/database/mysql/mayql.data.source.ts",
    "migration:revert": "cd database/mysql/migrations && NODE_ENV=migration ts-node ../../../node_modules/typeorm/cli.js migration:revert -d ../../../libs/common/src/database/mysql/mayql.data.source.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.0",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/terminus": "^10.2.0",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^10.3.0",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-redis": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cron": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.17.3",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "hbs": "^4.2.0",
    "ics": "^3.7.2",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.4",
    "multer": "^2.0.0-rc.4",
    "mysql2": "^3.7.0",
    "nats": "^2.19.0",
    "nodemailer": "^6.9.8",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sql-formatter": "^15.0.2",
    "typeorm": "^0.3.19",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "jest-html-reporter": "^3.10.2",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.2",
    "supertest": "^6.3.4",
    "ts-jest": "29.1.1",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1"
    },
    "reporters": [
      "default",
      [
        "jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ]
  }
}
